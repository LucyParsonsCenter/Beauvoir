<div class="container">
  <div class="row">
    <div class="span8">
      <table class="table">
	<tr>
	  <th>Title</th>
	  <th>Publisher(s)</th>
	  <th>Copies in stock</th>
	  <th>Copies sold</th>
	  <th>Copies on order</th>
	</tr>
	<% @titles.each do |title| %>
	<tr>
	  <td><%= link_to title.title,title%></td>
	  <td>TODO</td>
	  <td><%= title.in_stock %></td>
	  <td><%= title.sold %></td>
	  <td><%= title.on_order %></td>
	</tr>
	<% end %>
      </table>
    </div>
    <div class="span4">
      <%= render '/dashboard/search' %>
    </div>
  </div>
</div>

<h1>Listing post_title_list_links</h1>

<table>
  <tr>
    <th>Post</th>
    <th>Title list</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @post_title_list_links.each do |post_title_list_link| %>
  <tr>
    <td><%= post_title_list_link.post %></td>
    <td><%= post_title_list_link.title_list %></td>
    <td><%= link_to 'Show', post_title_list_link %></td>
